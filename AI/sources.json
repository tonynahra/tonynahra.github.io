{
  "providers": [
    {
      "id": "gemini",
      "name": "Google Gemini",
      "docs": "https://ai.google.dev/gemini-api/docs/models/gemini",
      "endpoint": "https://generativelanguage.googleapis.com/v1beta/models/{model}:generateContent?key={key}",
      "authType": "query",
      "models": ["gemini-2.5-flash","gemini-1.5-flash", "gemini-1.5-pro", "gemini-2.0-flash-exp"],
      "testBody": {
        "contents": [{ "parts": [{ "text": "Hello" }] }]
      }
    },
    {
      "id": "groq",
      "name": "Groq",
      "docs": "https://console.groq.com/docs/models",
      "endpoint": "https://api.groq.com/openai/v1/chat/completions",
      "authType": "bearer",
      "models": ["llama-3.3-70b-versatile", "llama-3.1-8b-instant", "mixtral-8x7b-32768"],
      "testBody": {
        "model": "{model}",
        "messages": [{ "role": "user", "content": "Hello" }]
      }
    },
    {
      "id": "openai",
      "name": "OpenAI",
      "docs": "https://platform.openai.com/docs/models",
      "endpoint": "https://api.openai.com/v1/chat/completions",
      "authType": "bearer",
      "models": ["gpt-4o", "gpt-4o-mini", "gpt-3.5-turbo"],
      "testBody": {
        "model": "{model}",
        "messages": [{ "role": "user", "content": "Hello" }]
      }
    },
    {
      "id": "anthropic",
      "name": "Anthropic",
      "docs": "https://docs.anthropic.com/en/docs/about-claude/models",
      "endpoint": "https://api.anthropic.com/v1/messages",
      "authType": "header-custom",
      "customHeaders": { 
        "x-api-version": "2023-06-01", 
        "anthropic-dangerously-allow-browser": "true" 
      },
      "models": ["claude-3-5-sonnet-latest", "claude-3-5-haiku-latest"],
      "testBody": {
        "model": "{model}",
        "max_tokens": 50,
        "messages": [{ "role": "user", "content": "Hello" }]
      }
    },
    {
      "id": "deepseek",
      "name": "DeepSeek",
      "docs": "https://api-docs.deepseek.com/",
      "endpoint": "https://api.deepseek.com/chat/completions",
      "authType": "bearer",
      "models": ["deepseek-chat", "deepseek-coder"],
      "testBody": {
        "model": "{model}",
        "messages": [{ "role": "user", "content": "Hello" }]
      }
    },
    {
      "id": "openrouter",
      "name": "OpenRouter",
      "docs": "https://openrouter.ai/models",
      "endpoint": "https://openrouter.ai/api/v1/chat/completions",
      "authType": "bearer",
      "models": ["google/gemini-2.0-flash-exp:free", "openai/gpt-4o", "meta-llama/llama-3.2-11b-vision-instruct:free"],
      "testBody": {
        "model": "{model}",
        "messages": [{ "role": "user", "content": "Hello" }]
      }
    }
  ]
}
